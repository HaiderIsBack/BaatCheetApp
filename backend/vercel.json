{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],"build": {
    "publish": [
      "public"
    ],
    "ignore": [
      "public/uploads/*"
    ]
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "./server.js"
        }
    ]
}
